{
  "name": "boil-extension",
  "displayName": "boil-extension",
  "description": "VSC extension that allows users to create and manage custom boilerpaltes.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "boil-extension.openTrackedTerminal",
        "title": "Open tracked terminal"
      },
      {
        "command": "boil-extension.viewHistory",
        "title": "View current recorded commands history"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./ && tsc-alias",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.105.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.36.0",
    "install": "^0.13.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3"
  }
}
